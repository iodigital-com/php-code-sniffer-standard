{
    "name": "iodigital-com/php-code-sniffer-standard",
    "conflict": {
        "isaac/php-code-sniffer": "*"
    },
    "replace": {
        "isaac/php-code-sniffer-standard": "*"
    },
    "description": "iO PHP_CodeSniffer Standard",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7 || ^1.0",
        "slevomat/coding-standard": "^8.15",
        "squizlabs/php_codesniffer": "^3.6.0",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IODigital\\CodingStandard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IODigital\\CodingStandard\\Tests\\": "tests/"
        },
        "files": [
            "vendor/squizlabs/php_codesniffer/autoload.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
